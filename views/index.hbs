{{>header}}
    {{#if pintores}}
    <table class="table table-dark table-hover">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nombre</th>
      <th scope="col">Nacionalidad</th>
      <th scope="col">Corriente</th>
      <th scope="col">Nacimiento</th>
       <th scope="col">Pintura</th>
    </tr>
  </thead>
  <tbody>
      {{#each pintores}}
    <tr> 
      <td>{{_id}}</td>
      <td>{{nombre}}</td>
      <td>{{nacionalidad}}</td>
      <td>{{estilo}}</td>
      <td>{{nacimiento}}</td>
      <td><img src="../images/{{pintura}}"/></td>
      
    </tr>
    {{/each}}
    
  </tbody>
</table>
{{else}}
<div>no se encuentran pintores</div>
{{/if}}
{{> footer}}
</body>
</html>